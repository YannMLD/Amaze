{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "dietOther": {
      "default": "",
      "type": "text"
    },
    "profilePicture": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "firstName": {
      "default": "",
      "type": "string"
    },
    "lastName": {
      "default": "",
      "type": "string"
    },
    "birthday": {
      "default": "",
      "type": "date",
      "required": true
    },
    "phone": {
      "default": "",
      "type": "string"
    },
    "organizedEvents": {
      "collection": "event",
      "dominant": true,
      "via": "organizers"
    },
    "invitedEvents": {
      "collection": "event",
      "dominant": true,
      "via": "guests"
    },
    "stuffs": {
      "collection": "stuff",
      "via": "usersOnIt",
      "dominant": true
    },
    "consumables": {
      "collection": "consumable",
      "via": "usersOnIt"
    },
    "diets": {
      "collection": "diet",
      "via": "users"
    }
  },
  "collectionName": "users-permissions_user"
}