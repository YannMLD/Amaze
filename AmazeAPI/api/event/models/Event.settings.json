{
  "connection": "default",
  "collectionName": "event",
  "info": {
    "name": "event",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "title": {
      "default": "",
      "maxLength": 50,
      "minLength": 1,
      "type": "string",
      "required": true
    },
    "date": {
      "default": "",
      "type": "date",
      "required": true
    },
    "date_isFinal": {
      "default": false,
      "type": "boolean",
      "required": true
    },
    "location": {
      "default": "",
      "type": "json",
      "required": true
    },
    "location_isFinal": {
      "default": false,
      "type": "boolean",
      "required": true
    },
    "organizers": {
      "collection": "user",
      "via": "organizedEvents",
      "plugin": "users-permissions"
    },
    "guests": {
      "collection": "user",
      "via": "invitedEvents",
      "plugin": "users-permissions"
    },
    "stuffs": {
      "collection": "stuff",
      "via": "event"
    },
    "consumables": {
      "collection": "consumable",
      "via": "event"
    },
    "entrancePrice": {
      "default": 0,
      "type": "decimal",
      "required": true
    }
  }
}